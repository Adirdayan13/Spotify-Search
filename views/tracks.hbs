<div class="header">
    <h1>Track list</h1>
    <h1>Preview</h1>
</div>

<div class="tracklist">
        {{#each results}}
        <div class="trackname">
            <p>{{this.track_number}}.  {{this.name}}</p>
        </div>
        <div class="audio">
            <audio class="audio" controls>
                <source src="{{this.preview_url}}">
            </audio>
        </div>
        {{/each}}
</div>
<div class="back">
    <button style="margin-bottom: 1%;"onclick="window.window.history.back();">Go back to albums</button>
</div>
